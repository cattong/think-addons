{
    "name": "cattong/think-addons",
    "description": "addons package for thinkphp5.1.x",
    "keywords": [
        "addons",
        "plugin",
        "thinkphp5",
        "framework"
    ],
    "homepage": "https://github.com/cattong/think-addons",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "beyongx.com",
            "email": "aronter@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/cattong/think-addons/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "topthink/think-helper": ">=1.0.4",
        "topthink/think-installer": ">=1.0.10"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think-config": {
            "addons": "src/config.php"
        }
    }
}
